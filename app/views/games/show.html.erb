<% content_for :head do %>
  <meta name="gameid" content="<%= @game.id %>">
  <meta name="finished" content="<%= @game.finished? %>">
  <meta name="started" content="<%= @game.started? %>">
<% end %>


<% if !@game.started? %>
<%= render partial: 'pending' %>
<% else %>
<%= render partial: 'two_up' %>
<% end %>


<script>
  TwoUp.start();
</script>
